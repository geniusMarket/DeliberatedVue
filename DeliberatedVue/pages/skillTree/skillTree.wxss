.tc {
  position: fixed;
  top: 0;
  height: 100%;
  width: 100%;
  background:rgba(255, 255, 255, 0.9);
  z-index: 900;
} 
.inner {
  background: #fff;
  margin: 0rpx 30rpx 0 30rpx;
  padding: 0 30rpx;
  height: 630rpx;
  border-radius: 10rpx;
  position: relative;
  top: 18%;
  z-index: 1000;
} 
.header {
  border-bottom: 1px solid #ccc;
  text-align: center;
  height: 300rpx;
  margin: 0 auto;
  line-height: 450rpx;
} 
.header image {
  width: 200rpx;
  height: 200rpx;
} 
.content {
  margin-left: 50rpx;
  margin-bottom: 70rpx;
} 
.content view {
  margin-top: 10rpx;
} 
.content text {
  display: block;
  color: #9d9d9d;
  margin-top: 20rpx;
} 
.bottom {
  border-radius: 80rpx;
  font-size: 35rpx;
  padding: 25rpx 10rpx; 
}